GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true
# Production API Endpoint
REACT_APP_API_URL=https://api.yourdomain.com

# Firebase Production Configuration
REACT_APP_FIREBASE_API_KEY=your-production-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-production-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id

# Google OAuth (for the frontend)
REACT_APP_GOOGLE_CLIENT_ID=your-production-google-client-id

# Application Settings
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true